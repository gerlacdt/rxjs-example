{
  "name": "rxjs-example",
  "version": "0.1.0",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "prettier-full": "prettier --single-quote --trailing-comma all --write 'src/**/*.ts'",
    "test": "NODE_ENV=test mocha --require source-map-support/register --recursive dist"
  },
  "dependencies": {
    "chai": "^4.1.2",
    "lodash": "4.17.4",
    "mocha": "^4.0.1",
    "rxjs": "^5.5.2",
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "prettier": "1.7.4",
    "tslint": "5.7.0",
    "tslint-config-prettier": "1.5.0",
    "typescript": "2.5.3"
  }
}
